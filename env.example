# AI 서비스 API 키 설정
# Google AI (Gemini) API 키
REACT_APP_GEMINI_API_KEY=your-gemini-api-key-here

# OpenAI API 키
REACT_APP_OPENAI_API_KEY=your-openai-api-key-here

# Anthropic API 키 (향후 지원 예정)
REACT_APP_ANTHROPIC_API_KEY=your-anthropic-api-key-here

# 데이터베이스 설정
REACT_APP_DATABASE_PATH=./data/storyboard.db

# 개발 환경 설정
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG_MODE=true

# API 기본 설정
REACT_APP_DEFAULT_AI_PROVIDER=google
REACT_APP_MAX_RETRY_ATTEMPTS=3
REACT_APP_REQUEST_TIMEOUT=30000

# 이미지 생성 설정
REACT_APP_DEFAULT_IMAGE_SIZE=1024x1024
REACT_APP_MAX_IMAGES_PER_REQUEST=4

# 영상 생성 설정
REACT_APP_DEFAULT_VIDEO_DURATION=5
REACT_APP_MAX_VIDEO_DURATION=60